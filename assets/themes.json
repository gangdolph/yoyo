      "--fg": "#f8f9fa",
      "--accent": "#ff71ce",
      "--gradient": "linear-gradient(135deg, #ff71ce 0%, #01cdfe 100%)"
    }
  },
  "dark": {
    "label": "Dark",
    "vars": {
      "--bg": "#1b1135",
      "--fg": "#e0d9f6",
      "--accent": "#01cdfe",
      "--gradient": "linear-gradient(135deg, #2d1e59 0%, #09002e 100%)"
    }
  },
  "vaporwave": {
    "label": "Vaporwave",
    "vars": {
      "--bg": "rgba(0, 0, 0, 0.3)",
      "--fg": "#ffffff",
